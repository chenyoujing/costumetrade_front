<view class="modal2_cover" bindtap="brand_modal_close" animation="{{brand_modal_opacity}}" style="display:{{brand_modal}};"></view>
<view class="modal2" animation="{{brand_modal_opacity}}" style="display:{{brand_modal}};">
  <view class="modal_header">搜索图片</view>
  <image src="../../../../images/close.png" mode="aspectFit" class="image_close" />
  <view class="search">
    <input class="title_input" type="text" name="search" placeholder="输入文件名" bindinput="bindKeyInput" />
    <image mode="aspectFit" src="../../../../images/search.png" class="image_search" />
  </view>

  <checkbox-group class="image_content" bindchange="checkboxChange">

    <label class="image_container">
      <image src=""></image>
      <checkbox value="" checked=""/>
    </label>
    <label class="image_container">
      <image src=""></image>
      <checkbox value="" checked=""/>
    </label>
    <label class="image_container">
      <image src=""></image>
      <checkbox value="" checked=""/>
    </label>

  </checkbox-group>
  <button class="image_submit">确定</button>
</view>

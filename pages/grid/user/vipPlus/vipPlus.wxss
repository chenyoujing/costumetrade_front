.vipPlus_title{
  background-color: #52CAC1;
  color: #ffffff;
  text-align: center;
  font-size: 16px;
  line-height: 50px;
  height: 50px;
  position: relative;
}
.vipPlus_title image{
  width: 16px;
  height: 16px;
  padding: 17px 10px;
  position: absolute;
  left: 0;
  transform: rotate(180deg);
}


.vip_bottom{
  background-color: #ffffff;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  height: 50px;
}
.bottom_info{
  border: 1px solid #B9CDCB;
  height: 48px;
  margin-right: 100px;
}
.bottom_check{
  float: left;
  font-size: 16px;
  width: 35%;
  height: 48px;
  line-height: 48px;
  text-align: center;
}
.bottom_check checkbox{
  margin-right: 10px;
}
.bottom_price{
  color: #ff0000;
  font-size: 16px;
  width: 65%;
  height: 48px;
  text-align: center;
  display: table-cell;
  vertical-align: middle;
}
.bottom_price::before{
  content: "总金额:";
  color: #000000;
}
.bottom_button{
  background-color: #FE2D04;
  color: #ffffff;
  float: right;
  font-size: 20px;
  line-height: 50px;
  height: 50px;
  width: 100px;
  text-align: center;
}
.customer_title{
  font-size: 18px;
  line-height: 30px;
  text-align: center;
}
.update_name,.update_level{
  padding: 10px 20px;
  border-bottom: 5px solid #DBF4F1;
}
.name_text{
  float: left;
  font-size: 16px;
  line-height: 30px;
  width: 40%;
}
.name_input{
  padding: 0 10px;
  border: 1px solid #52CAC1;
  font-size: 16px;
  line-height: 28px;
  height: 28px;
  margin-right: 20%;
}
.level_title{
  float: left;
  font-size: 16px;
  line-height: 30px;
  width: 40%;
}
.level_text{
  font-size: 12px;
  line-height: 30px;
  text-decoration: underline;
}
.slider_view{
  float: left;
  width: 20%;
  text-align: center;
  font-size: 12px;
  line-height: 40px;
  color: #999999;
}
.slider_checked{
  color: #000000;
}
.update_discount{
  padding: 10px 20px;
}
.discount_title{
  font-size: 16px;
  line-height: 50px;
}
.discount_table{
  margin: 0 20px;
}
.table_title1,.table_title2{
  box-sizing: border-box;
  float: left;
  width: 60%;
  background-color: #52CAC1;
  color: #ffffff;
  font-size: 18px;
  line-height: 30px;
  border: 1px solid #E3E3E3;
  text-align: center;
}
.table_text1,.table_text2{
  box-sizing: border-box;
  float: left;
  width: 60%;
  font-size: 18px;
  line-height: 40px;
  border: 1px solid #E3E3E3;
  text-align: center;
}
.table_title2,.table_text2{
  width: 40%;
}

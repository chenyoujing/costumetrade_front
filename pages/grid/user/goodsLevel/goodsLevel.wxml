<view class="level_title">设置货品级别折扣率</view>
<navigator open-type="navigateBack">
  <image src="../../../../images/close.png" mode="aspectFit" class="title_image_close" />
</navigator>

<view class="table clearfix">
  <view class="table_text bg_1 one_line">货品级别</view>
  <view class="table_text bg_2 one_line">普通会员</view>
  <view class="table_text bg_2 one_line">银卡会员</view>
  <view class="table_text bg_2 one_line">金卡会员</view>
  <view class="table_text bg_2 one_line">白金会员</view>
  <view class="table_text bg_2 one_line">钻石会员</view>
  <block wx:for="{{array}}">
    <view class="table_text bg_1">{{item.name}}</view>
    <view class="table_text">{{item.value[0]}}</view>
    <view class="table_text">{{item.value[1]}}</view>
    <view class="table_text">{{item.value[2]}}</view>
    <view class="table_text">{{item.value[3]}}</view>
    <view class="table_text">{{item.value[4]}}</view>
  </block>
</view>

<button class="submit">保存</button>

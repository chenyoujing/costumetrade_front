/*头部*/
@import "../../../../css/title.wxss";

.title_image_close{
  padding: 7px 10px;
}
.level_title{
  font-size: 18px;
  line-height: 30px;
  text-align: center;
}
.table{
  display: table;
  padding: 10px;
}
.table_title{
  box-sizing: border-box;
  background-color: #52CAC1;
  float: left;
  width: 25%;
  color: #ffffff;
  text-align: center;
  font-size: 14px;
  line-height: 30px;
  border: 1px solid #E3E3E3;
}
.table_text{
  box-sizing: border-box;
  float: left;
  width: 25%;
  color: #000000;
  text-align: center;
  font-size: 14px;
  line-height: 30px;
  border: 1px solid #E3E3E3;
}

.submit{
  font-size: 18px;
  border-radius: 0;
  background-color: #52CAC1;
  line-height: 40px;
  height: 40px;
  text-align: center;
  padding: 0;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  color: #ffffff;
}
